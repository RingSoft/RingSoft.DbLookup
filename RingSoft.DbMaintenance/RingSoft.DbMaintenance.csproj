<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;netstandard2.1</TargetFrameworks>
    <Version>1.11.00</Version>
    <Authors>Peter Ringering</Authors>
    <Description>A view model base class that retrieves, saves, and deletes a record in a database table.  Use with RingSoft.DbLookup.EfCore or RingSoft.DbLookup.Ef6.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://www.ringsoft.site</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RingSoft/RingSoft.DbLookup</RepositoryUrl>
    <PackageTags>Entity Framework, Database Table Maintenance</PackageTags>
    <PackageReleaseNotes>Changed WPF Lookup Window to be based off of a Control Template so that developers can customize its appearance and layout.  Added ability to subclass LookupDefinition to be compatible with the new Lookup Window Control Template.</PackageReleaseNotes>
    <PackageIcon></PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\RingSoft.DbLookup\RingSoft.DbLookup.csproj" />
  </ItemGroup>

</Project>
